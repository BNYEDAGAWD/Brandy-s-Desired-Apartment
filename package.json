{
  "name": "brandys-desired-apartment-finder",
  "version": "2.0.0",
  "description": "West Los Angeles luxury apartment search with automated search capabilities and intelligent ranking",
  "main": "index.html",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite serve --port 3000 --host",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .js,.ts,.html --fix",
    "lint:check": "eslint . --ext .js,.ts,.html",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "deploy": "npm run build && gh-pages -d dist",
    "serve": "http-server dist -p 8000 -c-1",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "lighthouse": "lhci autorun",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "deps:update": "npm update && npm audit fix",
    "ci:full": "npm run lint:check && npm run typecheck && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BNYEDAGAWD/Brandy-s-Desired-Apartment.git"
  },
  "keywords": [
    "apartment",
    "search",
    "west-los-angeles",
    "real-estate",
    "rental",
    "luxury",
    "github-pages",
    "javascript",
    "typescript",
    "progressive-web-app",
    "responsive",
    "vite",
    "modern"
  ],
  "author": "Brandy's Apartment Search Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BNYEDAGAWD/Brandy-s-Desired-Apartment/issues"
  },
  "homepage": "https://bnyedagawd.github.io/Brandy-s-Desired-Apartment/",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@lhci/cli": "^0.15.1",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-legacy": "^7.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^7.1.0",
    "gh-pages": "^6.1.0",
    "happy-dom": "^18.0.1",
    "http-server": "^14.1.1",
    "jsdom": "^23.2.0",
    "lighthouse": "^12.8.1",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2",
    "vite": "^7.0.6",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-pwa": "^1.0.2",
    "vitest": "^3.2.4",
    "workbox-window": "^7.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
