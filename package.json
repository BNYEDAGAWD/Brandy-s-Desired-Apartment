{
  "name": "brandys-desired-apartment-finder",
  "version": "2.0.0",
  "description": "West Los Angeles luxury apartment search with automated search capabilities and intelligent ranking",
  "main": "index.html",
  "type": "module",
  "scripts": {
    "dev": "vite serve --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build && gh-pages -d dist",
    "backend:install": "python3 -m pip install fastapi uvicorn pydantic python-dotenv httpx requests",
    "backend:start": "python3 start_deepsearch.py",
    "fullstack": "concurrently \"npm run backend:start\" \"npm run dev\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BNYEDAGAWD/Brandy-s-Desired-Apartment.git"
  },
  "keywords": [
    "apartment",
    "search",
    "west-los-angeles",
    "real-estate",
    "rental",
    "luxury",
    "github-pages",
    "javascript",
    "typescript",
    "progressive-web-app",
    "responsive",
    "vite",
    "modern"
  ],
  "author": "Brandy's Apartment Search Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BNYEDAGAWD/Brandy-s-Desired-Apartment/issues"
  },
  "homepage": "https://bnyedagawd.github.io/Brandy-s-Desired-Apartment/",
  "devDependencies": {
    "gh-pages": "^6.1.0",
    "vite": "^7.0.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "dependencies": {
    "concurrently": "^8.2.2",
    "node-fetch": "^3.3.2"
  }
}
